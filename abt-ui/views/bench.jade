html(lang='en')
	include head
		link(href='lib/codemirror/codemirror.css', rel='stylesheet')
		script(src="lib/codemirror/codemirror.js")
		script(src="lib/codemirror/mode/javascript/javascript.js")
	body
		include global-header
		div.container
			div.btn-group
				button.btn(id="undo") Undo
				button.btn(id="redo") Redo
				button.btn(id="script-btn") To Code
			div.btn-group
				button.btn(id="save") Save
		div(id="code-container")
			div(id="code-box")
		iframe(id="content-frame", style="width:100%; height:100%", src='/fetch?url=#{url}')
	include global_footer 
	script(src="/js/bench.js")